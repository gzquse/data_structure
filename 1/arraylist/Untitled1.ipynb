    "import math\n",
    "def competition(num):\n",
    "    if num == 1:\n",
    "        return 0\n",
    "    if num == 2:\n",
    "        return 1\n",
    "    else:\n",
    "        m, c = divmod(num,2)\n",
    "        return m + competition(c+m)\n"
